  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "setup": "node scripts/setup.js",
    "prerender": "node scripts/prerender.js",
    "lint:js": "eslint --ext .js,.mjs,.vue .",
    "fixlint:js": "eslint --ext .js,.vue . --fix",
    "format-check": "prettier --check \"**/*.(md|yaml|json)\"",
    "format": "prettier --write \"**/*.(md|yaml|json)\"",
    "yaml-validate:ranks": "ajv validate -s content/ranks.schema.json -c ./scripts/ajvExtraKeywords.js -d content/ranks.yaml",
    "yaml-validate:staff": "ajv validate -s content/staff.schema.json -c ./scripts/ajvExtraKeywords.js -d content/staff.yaml",
    "yaml-validate:commands": "ajv validate -s content/commands/commands.schema.json -c ./scripts/ajvExtraKeywords.js -d \"content/commands/**/*.yaml\"",
    "yaml-validate:locations": "ajv validate -s content/locations/locations.schema.json -c ./scripts/ajvExtraKeywords.js -d \"content/locations/**/*.yaml\"",
    "yaml-validate": "yarn run yaml-validate:ranks && yarn run yaml-validate:staff && yarn run yaml-validate:commands && yarn run yaml-validate:locations"
  },
