    "@splidejs/splide": "^4.1.4",
    "@unhead/vue": "^2.0.5",
    "bulma": "^1.0.3",
    "css.escape": "^1.5.1",
    "pinia": "^3.0.2",
    "query-string": "^9.1.1",
